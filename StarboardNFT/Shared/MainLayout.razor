@code {

    //bool bSignIn = false;
    UserProfileHeader objProfileHeaderSettings = new UserProfileHeader();

    public bool bProfileVisible = false;
    public bool bProfileItemClick = false;
    public bool bProfileMenuClick = false;
    public bool bUserClick = false;
    public bool bDarkTheme = false;

    string UsdAmount { get; set; }
    string EthAmount { get; set; }
    public string shortAddr { get; set; }
    public string searchText { get; set; }
    public byte[] newAuctionImg { get; set; }
    public int nUnreadNotification { get; set; }

    private HubConnection hubConnection;
    public Notification messageJsonString;

    public List<Notification> RecentNotification = new List<Notification>();

    // cookie
    public string selected_account_addr { get; set; }

    protected async override Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            var darkThemeSelected = await localStore.GetItemAsync<string>("darkSwitch") != null && await localStore.GetItemAsync<string>("darkSwitch") == "dark";
            if (darkThemeSelected)
            {
                bDarkTheme = true;
                await jsRuntime.InvokeVoidAsync("document.body.setAttribute", "data-theme", "dark");
            }
            else
            {
                bDarkTheme = false;
                await jsRuntime.InvokeVoidAsync("document.body.removeAttribute", "data-theme");
            }

            var profileheader_Id = await localStore.GetItemAsync<Guid>("headerId");
            string userId = profileheader_Id.ToString();

            objProfileHeaderSettings = await ProfileService.GetProfileAsync(profileheader_Id);

            hubConnection = new HubConnectionBuilder().WithUrl(NavigationManager.ToAbsoluteUri("/notificationHub?userId=" + userId)).Build();
            hubConnection.On<string>("NewAuctionNofitication", (nftDataList) =>
            {
                messageJsonString = JsonConvert.DeserializeObject<Notification>(nftDataList);

                RecentNotification.Add(messageJsonString);
                RecentNotification = RecentNotification.OrderByDescending(x => x.CreateDateTimeUtc).ToList();

                nUnreadNotification = GetCountOfUnreadNotification(RecentNotification).Result;

                StateHasChanged();
            });

            await hubConnection.StartAsync();

            selected_account_addr = await localStore.GetItemAsync<string>("address");
            StateHasChanged();

            if (!string.IsNullOrEmpty(selected_account_addr))
            {
                IEnumerable<Notification> enumerable = await NotificationService.GetRecentNotificationbyUserHeaderId(profileheader_Id);
                RecentNotification = enumerable.ToList();
                RecentNotification = RecentNotification.OrderByDescending(x => x.CreateDateTimeUtc).ToList();

                nUnreadNotification = await GetCountOfUnreadNotification(RecentNotification);

                await hubConnection.SendAsync("GetConnectionId");

                decimal ether_amount = await _ethereumHostProvider.GetAccountBalance(selected_account_addr);

                var usd_amount = Startup.CoinPriceDict["ETH"] * ether_amount;
                UsdAmount = Math.Round(usd_amount, 2) + "  USD";
                EthAmount = Math.Round((decimal)ether_amount, 4) + "  ETH";
            }
            else
                selected_account_addr = "";

            if (!string.IsNullOrEmpty(selected_account_addr))
            {
                shortAddr = selected_account_addr.Substring(0, 10) + "..." + selected_account_addr.Substring(38);
            }

            StateHasChanged();
        }
    }

    async Task<int> GetCountOfUnreadNotification(IEnumerable<Notification> RecentNotification)
    {
        int nUnread = 0;
        foreach (var recentNotification in RecentNotification)
        {
            if (recentNotification.IsRead == false)
                nUnread++;
        }

        return nUnread;
    }

    private async Task OnBellClick()
    {
        var profileheader_Id = await localStore.GetItemAsync<Guid>("headerId");
        await NotificationService.ReadNewNotifications(profileheader_Id);

        IEnumerable<Notification> enumerable = await NotificationService.GetRecentNotificationbyUserHeaderId(profileheader_Id);
        RecentNotification = enumerable.ToList();
        RecentNotification = RecentNotification.OrderByDescending(x => x.CreateDateTimeUtc).ToList();

        nUnreadNotification = await GetCountOfUnreadNotification(RecentNotification);

        StateHasChanged();
    }

    void OnUploadWork()
    {
        if (!string.IsNullOrEmpty(selected_account_addr))
            NavigationManager.NavigateTo("uploadnfttype");
        else NavigationManager.NavigateTo("signin");
    }

    void OnGoAuction()
    {
        if (!string.IsNullOrEmpty(selected_account_addr))
            NavigationManager.NavigateTo("explore");
        else NavigationManager.NavigateTo("signin");
    }

    void OnSignInClick()
    {
        NavigationManager.NavigateTo("signin");
    }

    void OnMyProfile()
    {
        NavigationManager.NavigateTo("account");
    }

    void OnViewProfile()
    {
        NavigationManager.NavigateTo("viewprofile");
    }

    void OnPortfolio()
    {
        NavigationManager.NavigateTo("portfolio");
    }

    private async Task OnSignOutClick()
    {
        await localStore.RemoveItemAsync("address");
        await localStore.RemoveItemAsync("ope_wallet_type");
        await localStore.RemoveItemAsync("open_wallet_date");

        NavigationManager.NavigateTo("signin", true);
    }

    void OnUserClick()
    {
        if (bProfileVisible == true) bProfileVisible = false;
        else bProfileVisible = true;

        bUserClick = true;
        StateHasChanged();
    }

    void OnOutsideClick()
    {
        if (bUserClick == true || bProfileMenuClick == true)
        {
            bUserClick = false;
            bProfileMenuClick = false;
        }
        else bProfileVisible = false;
        StateHasChanged();
    }

    void OnProfileMenuClick()
    {
        if (bProfileItemClick == false)
            bProfileVisible = true;
        else
        {
            bProfileVisible = false;
            bProfileItemClick = false;
        }

        bProfileMenuClick = true;
        StateHasChanged();
    }

    void OnProfileMenuItemClick()
    {
        bProfileItemClick = true;
    }

    private void OnViewAccountBalance()
    {
        NavigationManager.NavigateTo("account");
    }

    private async Task CopyTextToClipboard()
    {
        await jsRuntime.InvokeVoidAsync("blazorExtensions.copyText", selected_account_addr);
    }

    public void Enter(KeyboardEventArgs e)
    {
        if (e.Code == "Enter" || e.Code == "NumpadEnter")
        {
            NavigationManager.NavigateTo("search/" + searchText, true);
        }
    }

    private void SetValue(ChangeEventArgs e)
    {
        searchText = e.Value.ToString();
        StateHasChanged();
    }

    private async Task OnChangeTheme()
    {
        if (bDarkTheme)
        {
            bDarkTheme = false;
            await jsRuntime.InvokeVoidAsync("document.body.removeAttribute", "data-theme");
            await localStore.RemoveItemAsync("darkSwitch");
        }
        else
        {
            bDarkTheme = true;
            await jsRuntime.InvokeVoidAsync("document.body.setAttribute", "data-theme", "dark");
            await localStore.SetItemAsync("darkSwitch", "dark");
        }

        StateHasChanged();
    }

    private async Task OnChangeDarkToggle(ChangeEventArgs e)
    {
        if (bool.Parse(e.Value.ToString()))
        {
            bDarkTheme = true;
            await jsRuntime.InvokeVoidAsync("document.body.setAttribute", "data-theme", "dark");
            await localStore.SetItemAsync("darkSwitch", "dark");
        }
        else
        {
            bDarkTheme = false;
            await jsRuntime.InvokeVoidAsync("document.body.removeAttribute", "data-theme");
            await localStore.RemoveItemAsync("darkSwitch");
        }

        StateHasChanged();
    }
}
